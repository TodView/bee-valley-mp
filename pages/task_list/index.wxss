.wrap_item{
    width: 90%;
    height: 100rpx;
    display: flex;
    margin: auto;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #f1f1f1;
}
.wrap_item .showPrice{
    font-size: 35rpx;
    color: #00CC88;
    font-family: 'PingFangSC';
    font-weight: bold;
}
.wrap_item .showText{
    color: #3C3C3C;
    font-size: 35rpx;
    font-family: 'PingFangSC';
}