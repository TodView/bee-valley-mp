.wrap_index{
    width: 100vw;
    height: 100vh;
    background-image:url('data:image/png;base64,');
    background-repeat:no-repeat; 
    background-size:100% 100%;
}

.wrap_index .showBtn{
    width: 50%;
    height: 110rpx;
    margin: auto;
    padding-top: 500rpx;
}

.wrap_index .showBtn button{
    border-radius: 0;
    background: #00CC88;
}

.wrap_index .showIndex{
    width: 55%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    margin: auto;
}

.wrap_index .showIndex .showInfo{
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    margin-top: 280rpx;
}

.wrap_index .showIndex .showInfo image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
}
.wrap_index .showIndex .showInfo text{
    margin-top: 10rpx;
    text-align: center;
    font-size: 35rpx;
}

.wrap_index .showIndex .showBtn1{
    width: 90%;
    height: 110rpx;
    margin:auto;
    margin-top: 150rpx;
}
.wrap_index .showIndex .showBtn1 button{
    border-radius: 0;
    background: #00CC88;
}

.wrap_index .showIndex .showBtn2{
    width: 90%;
    height: 110rpx;
    margin:auto;
    margin-top: 50rpx;
}

.wrap_index .showIndex .showBtn2 button{
    border-radius: 0;
    background: #00CC88;
}